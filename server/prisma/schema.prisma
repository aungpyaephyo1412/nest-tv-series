generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum Role {
  Admin
  User
}

model User {
  id        String   @id @default(uuid())
  name      String
  email     String   @unique
  username  String   @unique @default(cuid())
  password  String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
